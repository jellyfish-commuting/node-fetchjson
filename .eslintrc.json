{
  "env":{
    "es6": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "semi": "error",
    "func-names": ["error", "never"],
    "arrow-parens": ["error", "as-needed"],
    "no-underscore-dangle": 0,
    "prefer-destructuring": ["error", {"object": true, "array": false}]
  }
}
